public class Solution {
    public ArrayList<Integer> solve(ArrayList<Integer> A) {
        ArrayList<Integer> arr=new ArrayList<>();
        int zero=0;
        for(int i:A){
            if(i>0){
                arr.add(i);
            }
            else{
                zero++;
            }
        }
        for(int j=0;j<zero;j++){
            arr.add(0);
        }
        return arr;
    }
}
