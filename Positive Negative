public class Solution {
    public ArrayList<Integer> solve(ArrayList<Integer> A) {
        ArrayList<Integer> tot=new ArrayList<>();
        int pos=0;
        int neg=0;
        for(int i:A){
            if(i>0){
                pos++;
            }
            if(i<0){
                neg++;
            }
        }
        tot.add(pos);
        tot.add(neg);
        return tot;
    }
}
