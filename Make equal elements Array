public class Solution {
    public int solve(int[] A, int B) {
        int ans=0;
        boolean flag=true;
        if(B==1){
            ans=1;
        }
    
        else{
            for(int i:A){
                if(i%B!=0){
                    flag=false;
                    ans=0;
                    break;
                }
            }
        }
        return ans;
    }
}
